User Function MyFunction()                        
Local btAlterar
Local btExcluir
Local btIncluir
Local cCliente
Local oFont1 := TFont():New("MS Sans Serif",,016,,.F.,,,,,.F.,.F.)
Local oFont1Ob := TFont():New("Arial Narrow",,018,,.T.,,,,,.F.,.F.)
Local ogCodigo
Local cgCodigo := "Define variable value"
Local oGet1
Local cGet1 := "Define variable value"
Local ogFantasia
Local cgFantasia := "Define variable value"
Local ogLoja
Local cgLoja := "Define variable value"
Local ogNome
Local cgNome := "Define variable value"
Local ogRadio                               
Local ngRadio := 1
Local oGroup1
Local oGroup2
Local oLoja
Local oNome
Local oSay1
Local oSay2
Static oDlg

  DEFINE MSDIALOG oDlg TITLE "Cliente" FROM 000, 000  TO 250, 600 COLORS 0, 16777215 PIXEL

    @ 005, 009 GROUP oGroup2 TO 103, 290 PROMPT "Clientes" OF oDlg COLOR 128, 16777215 PIXEL
    @ 040, 207 GROUP oGroup1 TO 077, 275 PROMPT "Bloqueado" OF oDlg COLOR 128, 16777215 PIXEL
    @ 023, 022 SAY cCliente PROMPT "Cliente:" SIZE 025, 007 OF oDlg FONT oFont1Ob COLORS 128, 16777215 PIXEL
    @ 023, 048 MSGET ogCodigo VAR cgCodigo SIZE 048, 010 OF oDlg COLORS 0, 16777215 F3 "SB1" PIXEL
    @ 023, 127 MSGET ogLoja VAR cgLoja SIZE 052, 010 OF oDlg COLORS 0, 16777215 PIXEL
    @ 023, 107 SAY oLoja PROMPT "Loja:" SIZE 017, 008 OF oDlg FONT oFont1Ob COLORS 128, 16777215 PIXEL
    @ 045, 025 SAY oNome PROMPT "Nome:" SIZE 017, 008 OF oDlg FONT oFont1Ob COLORS 128, 16777215 PIXEL
    @ 044, 048 MSGET ogNome VAR cgNome SIZE 141, 010 OF oDlg COLORS 0, 16777215 PIXEL
    @ 064, 049 MSGET ogFantasia VAR cgFantasia SIZE 141, 010 OF oDlg COLORS 0, 16777215 PIXEL
    @ 065, 018 SAY oSay1 PROMPT "Fantasia:" SIZE 027, 008 OF oDlg FONT oFont1Ob COLORS 128, 16777215 PIXEL
    @ 052, 217 RADIO ogRadio VAR ngRadio ITEMS "SIM","NÃO" SIZE 028, 018 OF oDlg COLOR 0, 16777215 PIXEL
    @ 106, 057 BUTTON btIncluir PROMPT "Incluir" SIZE 041, 016 OF oDlg PIXEL
    @ 105, 130 BUTTON btAlterar PROMPT "Alterar" SIZE 041, 016 OF oDlg PIXEL
    @ 106, 204 BUTTON btExcluir PROMPT "Excluir" SIZE 041, 016 OF oDlg PIXEL
    @ 084, 017 SAY oSay2 PROMPT "Fantasia:" SIZE 027, 008 OF oDlg FONT oFont1Ob COLORS 128, 16777215 PIXEL
    @ 082, 048 MSGET oGet1 VAR cGet1 SIZE 141, 010 OF oDlg COLORS 0, 16777215 PIXEL

  ACTIVATE MSDIALOG oDlg CENTERED

Return