#Include 'Protheus.ch'


User Function tFun1()

	Local cNome := "Nome declarado"
	Local nIdade := 50
	
	fNome2(cNome, nIdade)

Return()

// ---------------------------------------------------------------

Static Function fNome2(pNome,pIdade)
	
	pNome += " Sobrenome"
	
	MsgInfo("Nome: " + pNome + " Idade: " + cValToChar(pIdade))

Return( NIL )


// ***************************************************************

User Function tFun()

	Local cNome := fNome()
	
	MsgInfo("Retorno da funcao: " + cNome)

Return ( NIL )

// ---------------------------------------------------------------

Static Function fNome()

	Local cRet := "NOMDE DECALRADO NA FUNC"
	
Return ( cRet )

// ***************************************************************