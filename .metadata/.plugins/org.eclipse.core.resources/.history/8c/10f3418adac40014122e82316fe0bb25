#Include 'Protheus.ch'

User Function xTela()

	Local cTitulo := "Parametro"
	Local oDlg := Nil
	//Local cTexto := OEMTOANSI(“Total da Nota”)
	Local cTexto := "MV_DATAFIS"

	DEFINE MSDIALOG oDlg TITLE cTitulo FROM 000,000 TO 200,450 PIXEL
	ACTIVATE MSDIALOG oDlg CENTERED
	@ 005,010 SAY cTexto SIZE 55,07 OF oDlg PIXEL
	
	

Return ( NIL )

