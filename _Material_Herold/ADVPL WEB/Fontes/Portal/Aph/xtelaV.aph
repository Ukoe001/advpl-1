		<INPUT TYPE="submit" VALUE="GRAVAR" NAME="SALVAR" onclick="javascript:ok()">
		<INPUT TYPE="submit" VALUE="CANCELAR" NAME="SAIR" onClick="javascript:cancelar()">
	</FORM>
